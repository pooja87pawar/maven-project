pipeline
{
       agent any
       stages{
               stage('SCM Checkout'){
                                     steps {
                                       git 'https://github.com/pooja87pawar/maven-project.git'
                                           }
                                      }
                                      
                                      
                      stage('maven')
                              {
                               steps{
                                  withMaven(maven: 'mymaven') {
                                      sh 'mvn clean install package'
                                                 }
                                      
                                      }
                                  }
                                  
                                  
                       stage('sonar')
                              {
                               steps{
                               
                               withSonarQubeEnv('sonarnew') {
                                sh 'clean install sonar:sonar'
  
                                                            }
                               
                                      }
                                  }

                                      
                      
                     }
                     
            }

      
